{
  "name":"Plant",
  "type": "object",
  "properties": {
    "common_name": {
      "type": "string",
      "description": "Common name of the plant"
    },
    "species_name": {
      "type": "string",
      "description": "Scientific species name of the plant"
    },
    "description": {
      "type": "string",
      "description": "A brief description of the plant species"
    },
    "image_url": {
      "type": "string",
      "description": "URL of the user-uploaded image of the plant"
    },
    "health_status": {
      "type": "string",
      "enum": [
        "Healthy",
        "Needs Attention",
        "Diseased"
      ],
      "description": "Overall health assessment"
    },
    "health_analysis": {
      "type": "object",
      "properties": {
        "issues": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "issue": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "confidence": {
                "type": "number"
              }
            }
          }
        },
        "recommendations": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "description": "Detailed health analysis from the AI"
    },
    "care_tips": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "General care instructions for the plant species"
    }
  },
  "required": [
    "common_name",
    "species_name",
    "health_status"
  ]
}








































































































































































































































































































































































































































































































































































































































